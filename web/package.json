{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite",
    "build": "bun run typecheck && bun vite build --mode production",
    "preview": "bunx --bun vite preview --port 4173",
    "typecheck": "bun vue-tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,css,scss}\"",
    "generate-types": "cd .. && go run github.com/gzuidhof/tygo@latest generate",
    "update": "bun update --latest"
  },
  "dependencies": {
    "@types/nprogress": "^0.2.3",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "signature_pad": "^5.1.1",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tailwindcss/vite": "^4.1.16",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass": "^1.93.2",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.2"
  }
}