# goSign configuration (environment variables)
# Copy to .env and set values, or export before running.

# HTTP server address
GOSIGN_HTTP_ADDR=0.0.0.0:8088

# Development mode (true/false, 1/0, yes/no)
GOSIGN_DEV_MODE=false

# PostgreSQL connection URL
GOSIGN_POSTGRES_URL=postgres://user:password@localhost:5432/gosign?pool_max_conns=10

# Redis address and password
GOSIGN_REDIS_ADDRESS=localhost:6379
GOSIGN_REDIS_PASSWORD=
